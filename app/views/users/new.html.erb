<div id="form">
  <%= form_for @user do |f| %>
  <%= f.label 'Name' %>
  <%= f.text_field :name %>
  <%= f.label 'Email' %>
  <%= f.text_field :email_address %>
  <%= f.label 'Password' %>
  <%= f.text_field :password %>
  <%= f.label 'Password Confirmation' %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit value: 'Create My Account'%>
  <% end %>
</div>
